{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://thirdrevolution-staging.herokuapp.com/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Sign In"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_email"
      },
      "text": "jyothi_kandula@yahoo.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "user_password"
      },
      "text": "jyothi"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='signin-body']/form/input"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "q"
      },
      "text": "cars"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "commit"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "54 projects"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "nav"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "logout"
      }
    }
  ]
}